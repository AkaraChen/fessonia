{
  "name": "@tedconf/fessonia",
  "version": "v0.0.6",
  "description": "A node module easing the burden of automating ffmpeg from node.js",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/nyc --reporter=clover --reporter=lcov --reporter=text ./node_modules/.bin/mocha --reporter spec --level info test/*.test.js",
    "lint": "./node_modules/.bin/eslint ./{lib,test}/**/*.js",
    "watch": "NODE_ENV=test ./node_modules/.bin/nyc --reporter=clover --reporter=lcov --reporter=text ./node_modules/.bin/mocha --reporter spec --level info --watch test/*.test.js",
    "generate-docs": "scripts/generate-docs.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tedconf/fessonia.git"
  },
  "keywords": [
    "fessonia",
    "ffmpeg",
    "filtergraph",
    "node",
    "javascript",
    "nodejs",
    "video"
  ],
  "engines": {
    "node": ">=8.0"
  },
  "directories": {
    "lib": "./lib",
    "test": "./test",
    "doc": "./doc/tutorials"
  },
  "author": "Ryan B. Harvey <ryan.b.harvey@ted.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tedconf/fessonia/issues"
  },
  "homepage": "https://tedconf.github.io/fessonia",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.3",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.1.1",
    "tui-jsdoc-template": "^1.2.2"
  },
  "dependencies": {}
}
