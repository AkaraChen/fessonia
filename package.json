{
  "name": "ffmpeg-filtergraph",
  "version": "0.0.1",
  "description": "A node module for modeling and working with FFmpeg filtergraphs",
  "main": "index.js",
  "watch": {
    "test": "NODE_ENV=test ./node_modules/.bin/nyc --reporter=clover --reporter=lcov --reporter=text ./node_modules/.bin/mocha --reporter spec test/*.test.js",
    "lint": "./node_modules/.bin/eslint ./{lib,test}/**/*.js"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/nyc --reporter=clover --reporter=lcov --reporter=text ./node_modules/.bin/mocha --reporter spec test/*.test.js",
    "lint": "./node_modules/.bin/eslint ./{lib,test}/**/*.js",
    "testlint": "npm test && npm lint",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tedconf/ffmpeg-filtergraph.git"
  },
  "keywords": [
    "ffmpeg",
    "filtergraph"
  ],
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "author": "Ryan Harvey <ryan.b.harvey@ted.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tedconf/ffmpeg-filtergraph/issues"
  },
  "homepage": "https://github.com/tedconf/ffmpeg-filtergraph#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "npm-watch": "^0.6.0",
    "nyc": "^14.1.1",
    "sinon": "^7.1.1"
  },
  "dependencies": {}
}
